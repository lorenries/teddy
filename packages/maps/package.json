{
  "name": "@newamerica/maps",
  "version": "0.0.2",
  "description": "Nothing is certain in this life but death, taxes and requests for geographic data to be represented on a map",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/newamericafoundation/teddy.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "start": "rollup -c -w --environment BUILD:development",
    "build": "rollup -c --environment BUILD:production",
    "prepublish": "rm -rf ./dist && npm run build",
    "docs": "cd ./docs && ../../../node_modules/.bin/react-docgen ../src/**  | ../../../scripts/buildDocs.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vx",
    "react",
    "d3",
    "maps",
    "data",
    "visualization"
  ],
  "dependencies": {
    "@newamerica/charts": "^0.0.3",
    "@vx/group": "0.0.183",
    "@vx/scale": "0.0.182",
    "@vx/text": "0.0.183",
    "d3-array": "^1.2.4",
    "d3-collection": "^1.0.7",
    "d3-geo": "^1.11.3",
    "d3-interpolate": "^1.3.2",
    "prop-types": "^15.6.2",
    "topojson-client": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.5",
    "node-sass": "^4.9.2",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^1.6.3",
    "rollup-plugin-terser": "^4.0.2"
  },
  "author": "lorenries",
  "license": "MIT"
}
